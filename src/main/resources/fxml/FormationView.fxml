<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="5.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
        <columns>
          <TableColumn prefWidth="75.0" text="Desde" />
          <TableColumn prefWidth="75.0" text="Hasta" />
          <TableColumn prefWidth="75.0" text="Denominación" />
          <TableColumn prefWidth="75.0" text="Organizador" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <VBox prefHeight="400.0" prefWidth="70.0" spacing="5.0">
         <children>
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="59.0" text="Añadir" />
            <Button mnemonicParsing="false" text="Eliminar" />
         </children>
      </VBox>
   </children>
</HBox>
